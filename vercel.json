{
  "version": 2,
  "name": "uniflow-admin-console",
  "builds": [
    {
      "src": "admin-console/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/admin-console/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "admin-console/app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}